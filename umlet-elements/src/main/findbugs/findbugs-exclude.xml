<FindBugsFilter>
  <Match>
    <Package name="~com.baselet.element.sequence_aio.facet.*" />
  </Match>
  <Match>
    <Package name="~com.baselet.element.facet.customdrawings.gen.*" />
  </Match>
	<Match>
		<!-- singleton pattern -->
		<Class name="com.baselet.control.config.SharedConfig" />
		<Bug pattern="MS_EXPOSE_REP" />
	</Match>
    <Match>
        <Bug code="EI, EI2"/>
    </Match>
</FindBugsFilter>
